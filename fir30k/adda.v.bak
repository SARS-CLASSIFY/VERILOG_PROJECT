`timescale 1ns/1ps
module adda_test(
	input clk,//main clock
	
	output daclk,
	output   [7:0] dadata,//DAdata
	
	output adclk,
	input [7:0] addata //ADdata

);

wire signed [8:0] filterout;
wire signed [8:0] filterin;

assign   filterin[7:0]=addata;
assign   dadata=filterout[7:0];



assign daclk=clk;
assign adclk=clk;




filter filter_inst(
	.clk(clk),
	.clk_enable(1'b1),
	.reset(1'b0),
	.filter_in(filterin),
	.filter_out(filterout)
);




endmodule